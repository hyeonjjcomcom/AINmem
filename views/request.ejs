                            <span>Dashboard</span>
                        </div>
                    </button>
                </div>
            </div>

            <!-- 네비게이션 메뉴 -->
            <div class="nav-menu-section">
                <div class="nav-menu">
                    <div class="nav-item" onclick="navigateToPage('/dashboard/get-started')">
                        <a href="/dashboard/get-started">
                            <div class="nav-icon">
                                <svg></svg>
                            </div>
                            <span>Get Started</span>
                        </a>
                    </div>
                    <div class="nav-item active" onclick="navigateToPage('/dashboard/requests')">
                        <a href="/dashboard/requests">
                            <div class="nav-icon">
                                <svg></svg>
                            </div>
                            <span>Requests</span>
                        </a>
                    </div>
                    <div class="nav-item" onclick="navigateToPage('/dashboard/memories')">
                        <a href="/dashboard/memories">
                            <div class="nav-icon">
                                <svg></svg>
                            </div>
                            <span>Memories</span>
                        </a>
                    </div>
                    <div class="nav-item" onclick="navigateToPage('/dashboard/graph-memory')">
                        <a href="/dashboard/graph-memory">
                            <div class="nav-icon">
                                <svg></svg>
                            </div>
                            <span>Graph Memory</span>
                        </a>
                    </div>
                    <div class="nav-item" onclick="navigateToPage('/dashboard/users')">
                        <a href="/dashboard/users">
                            <div class="nav-icon">
                                <svg></svg>
                            </div>
                            <span>Users</span>
                        </a>
                    </div>
                    <div class="nav-item" onclick="navigateToPage('/dashboard/api-keys')">
                        <a href="/dashboard/api-keys">
                            <div class="nav-icon">
                                <svg></svg>
                            </div>
                            <span>API Keys</span>
                        </a>
                    </div>
                    <div class="nav-item" onclick="navigateToPage('/dashboard/webhooks')">
                        <a href="/dashboard/webhooks">
                            <div class="nav-icon">
                                <svg></svg>
                            </div>
                            <span>Webhooks</span>
                        </a>
                    </div>
                    <div class="nav-item" onclick="navigateToPage('/dashboard/memory-exports')">
